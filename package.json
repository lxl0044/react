    "es6-promise-polyfill": "^1.2.0",
    "express": "^4.15.3",
    "immutable": "^3.8.1",
    "moment": "^2.17.1",
    "normalize.css": "^7.0.0",
    "qs": "^6.4.0",
    "react": "^0.14.9",
    "react-dom": "^0.14.9",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.5",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.0",
    "redux-thunk": "^2.2.0",
    "socket.io-client": "^1.7.4"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.0.1",
